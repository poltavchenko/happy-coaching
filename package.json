{
  "name": "parcel-html-template",
  "version": "0.0.1",
  "description": "Starter template for building static web apps with HTML partials, SASS, Tailwind and JavaScript.",
  "scripts": {
    "clean": "rimraf dist",
    "start": "yarn clean && parcel serve ./index.html",
    "build": "yarn clean && parcel build ./index.html",
    "serve": "parcel serve ./index.html",
    "lint:js": "eslint \"./src/scripts/**/*.{ts,js}\"",
    "lint:js:fix": "eslint \"./src/scripts/**/*.{ts,js}\" --fix",
    "prettify": "prettier --write  \"src/**/*.html\"",
    "lint:styles": "stylelint \"./src/styles/**/*.{css,scss}\"",
    "lint:styles:fix": "stylelint \"./src/styles/**/*.{css,scss}\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dmytro Poltavchenko",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@fancyapps/ui": "^5.0.20",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.27",
    "postcss-scss": "^4.0.1",
    "posthtml": "^0.16.6",
    "posthtml-component": "^1.1.0",
    "posthtml-expressions": "^1.11.1",
    "posthtml-img-autosize": "^0.1.6",
    "posthtml-modules": "^0.9.1",
    "sass": "^1.64.1",
    "sharp": "0.29.1",
    "stylelint": "^14.5.3",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "parcel": "^2.0.0",
    "postcss-cli": "^9.0.1",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.3",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^1.7.4",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "rimraf": "^3.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "tailwindcss": "^3.3.3"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "posthtml-modules": {
    "attribute": "src",
    "attributeAsLocals": true
  }
}
